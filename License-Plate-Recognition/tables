iCREATE TABLE IF NOT EXISTS tasks (
    task_id INT AUTO_INCREMENT,
    title VARCHAR(255) NOT NULL,
    start_date DATE,
    due_date DATE,
    status TINYINT NOT NULL,
    priority TINYINT NOT NULL,
    description TEXT,
    PRIMARY KEY (task_id)
)  EN

create table resiInfo(
	plt_no varchar(12),
	owner varchar(50),
	flat_no varchar(10),
	primary key(plt_no)
)
create table loginInfo(
	usr varchar(50),
	pass varchar(50),
	primary key(usr)
)
